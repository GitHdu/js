<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Document</title>
</head>
<script>
    var arr=[1,2,3,4,1,5,6,4,8];
    var newArr=[];
    function hasContain(arr,num){
        for(var i=0;i<arr.length;i++){
            if(arr[i]==num){
                return true;
            }
        }
        return false;
    }
    for(var i=0;i<arr.length;i++){
        if(!hasContain(newArr,arr[i])){
            newArr.push(arr[i]);
        }
    }
</script>
<body>
    
</body>
</html>
